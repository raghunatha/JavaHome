<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">	

	<bean id="springQuizMaster" class="com.java.SpringQuizMaster"></bean>
	<bean id="strutsQuizMaster" class="com.java.StrutsQuizMaster"></bean>
	<bean id="quizMasterService" class="com.java.QuizMasterService">
		<property name="quizMaster">
			<ref local="strutsQuizMaster" />
		</property>
	</bean>
</beans>